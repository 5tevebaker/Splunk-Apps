      <single>
        <search>
          <query>| tstats summariesonly=t prestats=t count FROM datamodel="pan_firewall" WHERE (nodename="log.threat" OR nodename="log.wildfire.malicious") log.action="blocked" OR log.action="dropped" GROUPBY _time span=7d
| tstats summariesonly=t prestats=t append=t count FROM datamodel="pan_endpoint" WHERE nodename="log.attacks" log.action="blocked" GROUPBY _time span=7d
| stats count BY _time</query>
          <earliest>-14d</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
